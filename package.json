{
  "name": "nano",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "babel-cli": "~6.3.17",
    "babel-preset-es2015": "~6.3.13",
    "babel-preset-stage-2": "~6.3.13",
    "bower": "~1.6.5",
    "grunt": "~0.4.5",
    "grunt-bower-concat": "~0.6.0",
    "grunt-browser-sync": "~2.1.3",
    "grunt-cli": "~0.1.13",
    "grunt-contrib-concat": "~0.5.1",
    "grunt-contrib-imagemin": "~0.9.4",
    "grunt-contrib-jshint": "~0.11.2",
    "grunt-contrib-stylus": "~0.22.0",
    "grunt-contrib-uglify": "~0.10.0",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-fontoptim": "0.1.0",
    "grunt-newer": "~1.1.1",
    "http-server": "~0.8.5",
    "remark": "~4.1.0",
    "remark-lint": "~3.0.0",
    "stylobuild": "~0.7.0",
    "sweet2": "~2.0.0",
    "tamia-grunt": "~0.5.1"
  },
  "scripts": {
    "build": "babel-node lib",
    "lint": "remark source",
    "start-en": "http-server public/en -p 4242 -o",
    "start-ru": "http-server public/ru -p 4243 -o",
    "deploy": "bower install && grunt deploy && npm run build",
    "make-debug-symlinks": "ln -s ../images public/en/images; ln -s ../build public/en/build; ln -s ../images public/ru/images; ln -s ../build public/ru/build;"
  }
