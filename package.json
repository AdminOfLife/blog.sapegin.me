{
  "name": "nano",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "babel-cli": "~6.7.5",
    "babel-eslint": "~5.0.0",
    "babel-preset-tamia": "~6.5.0",
    "chokidar-cli": "~1.2.0",
    "eslint": "~1.10.3",
    "eslint-config-tamia": "~1.2.0",
    "fledermaus": "~4.1.0",
    "minimist": "~1.2.0",
    "remark": "~4.2.0",
    "remark-lint": "~3.2.0",
    "tamia-build": "~0.3.1",
    "unist-util-visit": "~1.1.0"
  },
  "scripts": {
    "start": "tamia server --lang en & npm run build:watch",
    "start:ru": "tamia server --lang ru & npm run build:watch",
    "test": "npm run lint:js && npm run lint:md",
    "lint:js": "eslint js src",
    "lint:md": "remark --quiet source",
    "bundle": "tamia bundle",
    "build": "node lib",
    "build:watch": "chokidar templates source -c 'node lib'",
    "compile": "babel -d lib src",
    "compile:watch": "babel --watch -d lib src"
  },
  "dependencies": {
    "social-likes-next": "~1.0.0",
    "tamia": "~2.0.3"
  }
}
